{
  "compilerOptions": {
    "module": "NodeNext",
    "moduleResolution": "nodenext",
    "jsx": "react-jsx",
    "jsxImportSource": "@kitajs/html",
    "plugins": [{ "name": "@kitajs/ts-html-plugin" }],
    "paths": {
      "#views/*": ["./views/*.tsx"],
    },
    "experimentalDecorators": true
  },
}